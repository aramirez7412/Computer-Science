#include "Header.h"
#include "StudentClass.h"

void PropagateStudentArray(const int SIZE, Student stuAr[])
{
	cout << "Enter the proper information to continue."

	for(int index = 0; index < SIZE; index++)
	{
		cout << "Enter Name: " ;
		getline(cin, stuAr[index].SetName());
	}
}
